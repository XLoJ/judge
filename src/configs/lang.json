{
  "c": {
    "compile": {
      "cmd": "/usr/bin/gcc",
      "args": [
        "-static", "-O2", "-std=c99", "-DONLINE_JUDGE", "-lm", "-o", "{exe_file}", "{code_file}"
      ]
    },
    "code_file": "foo.c",
    "execute_prefix": "./",
    "exe_ext": "bin"
  },
  "cpp": {
    "compile": {
      "cmd": "/usr/bin/g++",
      "args": [
        "-static", "-O2", "-std=c++11", "-DONLINE_JUDGE", "-lm", "-o", "{exe_file}", "{code_file}"
      ]
    },
    "code_file": "foo.cpp",

    "exe_ext": "bin11"
  },
  "cc14": {
    "compile": {
      "cmd": "/usr/bin/g++",
      "args": [
        "-static", "-O2", "-std=c++14", "-DONLINE_JUDGE", "-lm", "-o", "{exe_file}", "{code_file}"
      ]
    },
    "code_file": "foo.cpp",

    "exe_ext": "bin14"
  },
  "cc17": {
    "compile": {
      "cmd": "/usr/bin/g++",
      "args": [
        "-static", "-O2", "-std=c++17", "-DONLINE_JUDGE", "-lm", "-o", "{exe_file}", "{code_file}"
      ]
    },
    "code_file": "foo.cpp",

    "exe_ext": "bin17"
  },
  "java": {
    
  },
  "py2": {

  },
  "python": {
    "compile": {
      "cmd": "/bin/cp",
      "args": [
        "{code_file}", "{exe_file}"
      ]
    },
    "code_file": "foo.py",

    "exe_ext": "py"
  }
}